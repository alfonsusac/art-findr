generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mitra {
  id String @id @default(cuid())
  name String
  image String
  availability String
  expertises String
  considerations String
  status StatusMitra
  harga String
  location String
}

enum StatusMitra {
  Tersedia
  Sibuk
}